<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="@drawable/background"
    android:tileMode="disabled"
    android:gravity="top"
    tools:ignore="HardcodedText">

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar_layout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="265dp"
        android:layout_height="81dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="72dp"
        android:layout_marginBottom="552dp"
        android:fontFamily="@font/atma_semibold"
        android:text="KAUNAS"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="60sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.065" />

    <Button
        android:id="@+id/news"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="45dp"
        android:layout_marginBottom="86dp"
        android:background="#80FFFFFF"
        android:fontFamily="@font/atma_semibold"
        android:text="Naujienos"
        android:textAlignment="inherit"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/hotels"
        app:layout_constraintTop_toBottomOf="@+id/quiz" />

    <Button
        android:id="@+id/hotels"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="21dp"
        android:layout_marginBottom="86dp"
        android:background="#80FFFFFF"
        android:fontFamily="@font/atma_semibold"
        android:text="Viešbučiai"
        android:textAlignment="inherit"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/news"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/restaurants" />

    <Button
        android:id="@+id/restaurants"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="21dp"
        android:layout_marginBottom="18dp"
        android:background="#80FFFFFF"
        android:fontFamily="@font/atma_semibold"
        android:text="Kur pavalgyti?"
        android:textAlignment="inherit"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/hotels"
        app:layout_constraintEnd_toStartOf="@+id/quiz"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/about" />

    <Button
        android:id="@+id/about"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="151dp"
        android:layout_marginEnd="21dp"
        android:layout_marginBottom="17dp"
        android:background="#80FFFFFF"
        android:fontFamily="@font/atma_semibold"
        android:text="Apie"
        android:textAlignment="inherit"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/restaurants"
        app:layout_constraintEnd_toStartOf="@+id/placestovisit"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/quiz"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="45dp"
        android:layout_marginBottom="18dp"
        android:background="#80FFFFFF"
        android:fontFamily="@font/atma_semibold"
        android:text="Pasitikrink    žinias"
        android:textAlignment="inherit"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/news"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/restaurants"
        app:layout_constraintTop_toBottomOf="@+id/placestovisit" />

    <Button
        android:id="@+id/placestovisit"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="151dp"
        android:layout_marginEnd="45dp"
        android:layout_marginBottom="17dp"
        android:background="#80FFFFFF"
        android:fontFamily="@font/atma_semibold"
        android:text="Lankytinos    vietos"
        android:textAlignment="inherit"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/quiz"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/about"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="174dp"
        android:layout_marginTop="46dp"
        android:layout_marginEnd="172dp"
        android:layout_marginBottom="85dp"
        android:text="Sniegas -3"
        app:layout_constraintBottom_toTopOf="@+id/about"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

</androidx.constraintlayout.widget.ConstraintLayout>


